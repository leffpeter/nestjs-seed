{
    "name": "nestjs-seed",
    "private": true,
    "version": "0.1.0",
    "description": "nestjs-seed",
    "license": "GPLv2",
    "scripts": {
        "prebuild": "rimraf dist",
        "build": "nest build",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "start": "nest start",
        "start:dev": "nest start --watch",
        "start:debug": "nest start --debug --watch",
        "start:prod": "node dist/main",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json",
        "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js"
    },
    "dependencies": {
        "@automapper/classes": "^6.1.4",
        "@automapper/core": "^6.1.4",
        "@automapper/nestjs": "^6.1.4",
        "@golevelup/ts-jest": "^0.3.2",
        "@jest/types": "^27.4.2",
        "@liaoliaots/nestjs-redis": "^5.1.0",
        "@nestjs/common": "^8.0.6",
        "@nestjs/config": "^1.0.1",
        "@nestjs/core": "^8.0.6",
        "@nestjs/jwt": "^8.0.0",
        "@nestjs/passport": "^8.0.1",
        "@nestjs/platform-express": "^8.0.6",
        "@nestjs/platform-socket.io": "^8.0.11",
        "@nestjs/platform-ws": "^8.2.2",
        "@nestjs/schedule": "^1.0.1",
        "@nestjs/serve-static": "^2.2.2",
        "@nestjs/swagger": "^5.0.9",
        "@nestjs/terminus": "^8.0.1",
        "@nestjs/throttler": "^2.0.0",
        "@nestjs/typeorm": "^8.0.2",
        "@nestjs/websockets": "^8.0.11",
        "@types/redis": "^2.8.32",
        "bcrypt": "^5.0.1",
        "cache-manager": "^3.4.4",
        "class-transformer": "^0.4.0",
        "class-validator": "^0.13.1",
        "cluster": "^0.7.7",
        "compression": "^1.7.4",
        "csurf": "^1.11.0",
        "dotenv": "^10.0.0",
        "helmet": "^4.6.0",
        "ioredis": "^4.28.0",
        "jest-mock": "^27.4.2",
        "lodash": "^4.17.21",
        "mysql2": "^2.3.0",
        "nestjs-redis": "^1.3.3",
        "passport": "^0.4.1",
        "passport-jwt": "^4.0.0",
        "passport-local": "^1.0.0",
        "pm2": "^5.1.2",
        "reflect-metadata": "^0.1.13",
        "rimraf": "^3.0.2",
        "rxjs": "^7.3.0",
        "socket.io-redis": "^6.1.1",
        "swagger-ui-express": "^4.1.6",
        "terminus": "^1.1.0",
        "typeorm": "^0.2.37"
    },
    "devDependencies": {
        "@automapper/types": "^6.1.4",
        "@babel/preset-typescript": "^7.16.0",
        "@compodoc/compodoc": "^1.1.15",
        "@nestjs/cli": "^8.1.1",
        "@nestjs/schematics": "^8.0.2",
        "@nestjs/testing": "^8.0.6",
        "@types/bcrypt": "^5.0.0",
        "@types/cache-manager": "^3.4.2",
        "@types/compression": "^1.7.2",
        "@types/cron": "^1.7.3",
        "@types/express": "^4.17.13",
        "@types/ioredis": "^4.28.1",
        "@types/jest": "^27.0.1",
        "@types/lodash": "^4.14.173",
        "@types/node": "^16.7.1",
        "@types/passport-jwt": "^3.0.6",
        "@types/passport-local": "^1.0.34",
        "@types/supertest": "^2.0.11",
        "@types/ws": "^8.2.0",
        "@typescript-eslint/eslint-plugin": "^5.5.0",
        "@typescript-eslint/parser": "^5.5.0",
        "eslint": "^7.32.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-config-standard": "^16.0.3",
        "eslint-plugin-import": "^2.25.3",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-prettier": "^3.4.1",
        "eslint-plugin-promise": "^5.2.0",
        "jest": "^27.0.6",
        "passport-custom": "^1.1.1",
        "prettier": "^2.3.2",
        "supertest": "^6.1.6",
        "ts-jest": "^27.0.5",
        "ts-loader": "^9.2.5",
        "ts-node": "^10.2.1",
        "tsconfig-paths": "^3.10.1",
        "typescript": "^4.3.5"
    },
    "moduleDirectories": [
        "node_modules",
        "src"
    ],
    "jest": {
        "collectCoverage": true,
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "modulePaths": [
            "<rootDir>"
        ],
        "testRegex": ".spec.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "coverageDirectory": "../coverage",
        "testEnvironment": "node",
        "verbose": false
    }
}
